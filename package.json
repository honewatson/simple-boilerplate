{
  "name": "babel-npm-boilerplate",
  "author": "Hone Watson",
  "version": "0.2.0",
  "description": "A Boilerplate for creating NPM packages using Babel, ESLint, Mocha and Chai",
  "main": "build/index.js",
  "keywords": [
    "es6",
    "babel",
    "eslint",
    "npm package"
  ],
  "scripts": {
    "test": "mocha --compilers js:babel-core/register --recursive",
    "test-cov": "babel-node isparta cover --report html --report text node_modules/.bin/_mocha",
    "build": "rollup -c rollup.config.js  --environment INCLUDE_DEPS,BUILD:production && babel rollup --out-dir build",
    "clean": "rm -rf build",
    "lint": "eslint src test"
  },
  "dependencies": {},
  "devDependencies": {
    "babel-core": "^6.5.2",
    "babel-eslint": "^5.0.0",
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "chai": "^3.5.0",
    "eslint": "^2.2.0",
    "eslint-config-airbnb": "^6.0.1",
    "eslint-plugin-react": "^4.0.0",
    "isparta": "^4.0.0",
    "mocha": "^2.4.5"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/honewatson/babel-npm-boilerplate.git"
  },
  "bugs": {
    "url": "https://github.com/honewatson/babel-npm-boilerplate/issues"
  },
  "homepage": "https://github.com/honewatson/babel-npm-boilerplate#readme",
  "license": "MIT"
}
