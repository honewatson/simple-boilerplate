{
  "name": "babel-npm-boilerplate",
  "author": "Hone Watson",
  "version": "0.2.0",
  "description": "A Boilerplate for creating NPM packages using Babel, ESLint, Mocha and Chai",
  "main": "build/index.js",
  "keywords": [
    "es6",
    "babel",
    "eslint",
    "npm package"
  ],
  "scripts": {
    "test": "mocha --compilers js:babel-core/register --recursive",
    "test-cov": "babel-node isparta cover --report html --report text node_modules/.bin/_mocha",
    "webpack-production": "webpack --progress -p --config webpack.production.config.js",
    "rollup-production": "rollup -c rollup.production.config.js --environment INCLUDE_DEPS,BUILD:production",
    "server": "node ./server.js",
    "clean": "rm -rf build",
    "flow": "flow check",
    "pretest": "npm run flow && npm run build",
    "lint": "eslint src test"
  },
  "dependencies": {
    "lodash.pick": "^4.2.1"
  },
  "devDependencies": {
    "autoprefixer": "^6.3.7",
    "babel-core": "^6.5.2",
    "babel-eslint": "^5.0.0",
    "babel-plugin-transform-flow-strip-types": "^6.8.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-es2015-rollup": "^1.1.1",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.9.0",
    "babelrc-rollup": "^2.0.0",
    "chai": "^3.5.0",
    "css-loader": "^0.23.1",
    "eslint": "^2.13.1",
    "eslint-config-airbnb": "^6.0.1",
    "eslint-plugin-react": "^4.0.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "flow-bin": "^0.29.0",
    "isparta": "^4.0.0",
    "mocha": "^2.4.5",
    "node-sass": "^3.8.0",
    "postcss-loader": "^0.9.1",
    "rollup-plugin-babel": "^2.6.1",
    "rollup-plugin-commonjs": "^3.3.0",
    "rollup-plugin-multi-entry": "^2.0.0",
    "rollup-plugin-node-resolve": "^1.7.1",
    "rollup-plugin-uglify": "^1.0.1",
    "sass-loader": "^4.0.0",
    "semantic-release": "^4.3.5",
    "style-loader": "^0.13.1",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1",
    "webpack-shell-plugin": "^0.4.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/honewatson/babel-npm-boilerplate.git"
  },
  "bugs": {
    "url": "https://github.com/honewatson/babel-npm-boilerplate/issues"
  },
  "homepage": "https://github.com/honewatson/babel-npm-boilerplate#readme",
  "license": "MIT"
}
